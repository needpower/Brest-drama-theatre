{
  "extends": ["airbnb", "plugin:jest/recommended"],
  "plugins": ["react"],
  "settings": {
    "import/resolver": {
      "babel-module": {},
      "node": {
        "paths": ["src"]
      }
    }
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
